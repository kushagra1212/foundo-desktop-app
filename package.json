    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@ngrx/effects": "^15.2.1",
    "@ngrx/store": "^15.2.1",
    "@ngrx/store-devtools": "^15.2.1",
